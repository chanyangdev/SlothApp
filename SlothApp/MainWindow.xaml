<Window x:Class="SlothApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Sloth (Clean Start)" Width="600" Height="300">
    <Grid Margin="16">
        <StackPanel>
            <TextBlock Text="Sloth clean start" FontSize="18" FontWeight="Bold" Margin="0,0,0,8"/>
            <Button x:Name="BtnTest" Content="Test Core Reference" Width="200" Height="34" Click="BtnTest_Click"/>
            <TextBlock x:Name="LblOut" Margin="0,12,0,0"/>
        </StackPanel>
    </Grid>
</Window>
